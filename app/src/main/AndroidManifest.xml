<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.reactive.fyp">

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:hardwareAccelerated="true"
        android:icon="@drawable/shirtlogo"
        android:label="@string/app_name"
        android:roundIcon="@drawable/shirtlogo"
        android:supportsRtl="true"
        android:theme="@style/Theme.FYP"
        tools:replace="android:icon">
        <activity android:name=".Activities.AboutActivity"></activity>
        <activity android:name=".Activities.OrderActivity" />
        <activity android:name=".Activities.EditprofileActivity" />
        <activity android:name=".Activities.CheckOutActivity" />
        <activity android:name=".Activities.HomeActivity" />
        <activity android:name=".Activities.ProfileActivity" />
        <activity android:name=".Activities.testActivity" />
        <activity android:name=".Activities.DesignActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Activities.SignInActivitye" />
        <activity android:name=".Activities.RegisterActivity" />
        <activity android:name=".Activities.ImageViewActivity" />
    </application>

</manifest>